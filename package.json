{
  "private": true,
  "scripts": {
    "start-client": "umi dev",
    "build-client": "umi build",
    "start-server": "ts-node-dev -r dotenv/config --project tsconfig.server.json ./src/server/index.ts",
    "build-server": "rimraf -rf lib && tsc --project tsconfig.server.json",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "dotenv": "^16.0.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
